{
  "name": "dismae-ui",
  "version": "0.0.3",
  "description": "User interface to help with dismae game development.",
  "main": "index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "lint": "standard --verbose",
    "pack": "build",
    "dist": "build",
    "app": "electron app",
    "build-all": "./node_modules/.bin/build --platform all --arch all --dist",
    "windows": "./node_modules/.bin/build --platform win32 --arch all --dist",
    "darwin": "./node_modules/.bin/build --platform darwin --arch all --dist",
    "linux": "./node_modules/.bin/build --platform linux --arch all --dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dischan/dismae-ui.git"
  },
  "author": "Dischan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dischan/dismae-ui/issues"
  },
  "homepage": "https://github.com/Dischan/dismae-ui#readme",
  "devDependencies": {
    "electron-builder": "^3.6.1",
    "electron-prebuilt": "^0.37.5"
  },
  "build": {
    "app-bundle-id": "co.dischan.dismae",
    "app-category-type": "public.app-category.developer-tools",
    "productName": "Dismae"
  }
}
